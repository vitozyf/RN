{
  "name": "ZnlICloud",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "postinstall": "sed -i '' 's/#import <RCTAnimation\\/RCTValueAnimatedNode.h>/#import \"RCTValueAnimatedNode.h\"/' ./node_modules/react-native/Libraries/NativeAnimation/RCTNativeAnimatedNodesManager.h",
    "build-android": "./gradlew assembleRelease",
    "install-ios-release": "react-native run-ios --configuration Release",
    "android-linux": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
    "bundle-android": "react-native bundle --platform android --entry-file index.js --bundle-output ./bundles/index.android.bundle --assets-dest ./bundles/assets/ --dev false",
    "bundle-ios": "node node_modules/react-native/local-cli/cli.js bundle --entry-file index.js --platform ios --dev false --bundle-output ./ios/bundle/index.ios.jsbundle --assets-dest ./ios/bundle",
    "publishdev": "code-push release-react ZnlICloud android --t 1.0.6 --dev false  --des '1.优化操作流程' --m false",
    "publishpro": "code-push release-react ZnlICloud android --t 1.0.4 --dev false --d Production --des '1.优化操作流程' --m false",
    "publish1": "code-push release ZnlICloud ./bundles/index.android.bundle 1.0.4 -d Production --des '1.0 正式API' -m true",
    "code-push promote": "code-push promote"
  },
  "dependencies": {
    "react": "16.5.0",
    "react-native": "0.57.2",
    "react-native-charts-wrapper": "git+https://github.com/wuxudong/react-native-charts-wrapper.git",
    "react-native-code-push": "^5.4.2",
    "react-native-datepicker": "^1.7.2",
    "react-native-device-info": "^0.24.3",
    "react-native-keyboard-aware-scroll-view": "^0.7.4",
    "react-native-modal": "^6.5.0",
    "react-native-root-siblings": "2.2.0",
    "react-native-splash-screen": "^3.1.1",
    "react-native-swiper": "^1.5.14",
    "react-native-vector-icons": "^6.0.2",
    "react-navigation": "^2.18.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.1.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "eslint": "^5.9.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "flow-bin": "0.78.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "^0.49.2",
    "react-test-renderer": "16.5.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
